{
  "name": "@thesonofthomp/monorepo",
  "private": true,
  "description": "Adam Thompson's monorepo",
  "version": "0.1.0",
  "author": "Adam Thompson <adam.m.thompson@icloud.com>",
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=10.18.0"
  },
  "scripts": {
    "build": "turbo build",
    "build:server": "pnpm run --filter=@thesonofthomp/web netlify build",
    "clean:modules": "rm -rf node_modules && pnpm exec rm -rf node_modules",
    "check": "turbo check",
    "dev": "turbo dev",
    "dev:server": "pnpm run --filter=@thesonofthomp/web netlify dev",
    "serve": "pnpm run build:server; pnpm run --filter=@thesonofthomp/web netlify serve"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.4",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.18.3"
}
